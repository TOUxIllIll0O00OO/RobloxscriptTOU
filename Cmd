local Players = game:GetService("Players")
local player = Players.LocalPlayer
local RunService = game:GetService("RunService")

-- ‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏™‡πà‡∏á‡∏ú‡∏•‡∏Å‡∏£‡∏∞‡∏ó‡∏ö
local whitelist = {
    "HRdrluOs1416",
    "birdV2_123",
    "AniF_Xx",
    -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ä‡∏∑‡πà‡∏≠‡∏≠‡∏∑‡πà‡∏ô‡πÜ ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà (‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î 5 ‡∏Ñ‡∏ô)
    "‡∏ä‡∏∑‡πà‡∏≠1",
    "‡∏ä‡∏∑‡πà‡∏≠2",
    "‡∏ä‡∏∑‡πà‡∏≠3",
    "‡∏ä‡∏∑‡πà‡∏≠4",
    "‡∏ä‡∏∑‡πà‡∏≠5"
}

-- ‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà‡∏™‡πà‡∏á‡∏ú‡∏• reset
local resetPos = Vector3.new(-265.01, 202.88, -248.89)
-- ‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà‡∏™‡πà‡∏á‡∏ú‡∏• kick
local kickPos = Vector3.new(-274.85, 202.88, -275.54)

-- ‡∏£‡∏∞‡∏¢‡∏∞‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
local threshold = 3 -- ‡∏õ‡∏£‡∏±‡∏ö‡∏ï‡∏≤‡∏°‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ trigger

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô whitelist ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
local function isWhitelisted(name)
    for _, v in ipairs(whitelist) do
        if v == name then
            return true
        end
    end
    return false
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏∏‡∏Å frame
RunService.RenderStepped:Connect(function()
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then return end

    local pos = player.Character.HumanoidRootPart.Position
    local name = player.Name

    if not isWhitelisted(name) then
        -- ‡πÄ‡∏ä‡πá‡∏Ñ reset
        if (pos - resetPos).Magnitude <= threshold then
            -- reset ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
            if player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.Health = 0
            end
        end

        -- ‡πÄ‡∏ä‡πá‡∏Ñ kick
        if (pos - kickPos).Magnitude <= threshold then
            player:Kick("‡πÄ‡∏ô‡πá‡∏ï‡∏´‡∏•‡∏∏‡∏î‡∏ô‡∏∞‡∏à‡πâ‡∏∞üòã")
        end
    end
end)
