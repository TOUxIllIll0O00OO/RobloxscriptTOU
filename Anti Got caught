ShowButton.Position = UDim2.new(0, 5, 0, 5)
ShowButton.BackgroundColor3 = Color3.fromRGB(50, 200, 100)
ShowButton.Text = "‡πÅ‡∏™‡∏î‡∏á GUI"
ShowButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ShowButton.Font = Enum.Font.FredokaOne
