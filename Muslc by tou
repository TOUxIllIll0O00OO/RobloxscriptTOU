            if spinEnabled then
                spawn(function()
                    while spinEnabled do
                        if fortuneWheelRemote and fortuneWheel then
                            local args = {"openFortuneWheel", fortuneWheel}
                            fortuneWheelRemote:InvokeServer(unpack(args))
                        else
                            spinToggle.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö Remote!"
                            task.wait(1)
